{
    "name": "escolalms/pages",
    "description": "Escola page repository API.",
    "type": "package",
    "require": {
        "php": ">=7.4",
        "laravel/framework": ">=8.0",
        "escolalms/core": "^1",
        "davidbadura/faker-markdown-generator": "=1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Chołaj",
            "email": "pawel.cholaj@escola.pl"
        },
        {
            "name": "Mariusz Gliwiński",
            "email": "mariusz.gliwinski@escola.pl"
        }
    ],
    "autoload": {
        "psr-4": {
            "EscolaLms\\Pages\\": "src",
            "EscolaLms\\Pages\\Database\\Seeders\\": "database/seeders",
            "Database\\Factories\\EscolaLms\\Pages\\Models\\": "database/factories",
            "EscolaLms\\Pages\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EscolaLms\\Pages\\EscolaLmsPagesServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
